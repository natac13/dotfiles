{
  "suggest.noselect": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "graphql",
    "css",
    "html"
  ],
  "typescript.validate.enable": true,
  "eslint.enable": true,
  "eslint.autoFixOnSave": true,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": false,
  "prettier.disableSuccessMessage": true,
  "prettier.arrowParens": "always",
  "explorer.icon.enableNerdfont": true,
  "tsserver.enable": true,
  "explorer.position": "left",
  "explorer.autoCollapseOptions": ["recursive"],
  "explorer.width": 35,
  "explorer.file.hiddenRules": {
    "extensions": ["o", "a", "obj", "pyc"],
    "filenames": ["node_modules"],
    "patternMatches": ["^\\."]
  },
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ]
}
